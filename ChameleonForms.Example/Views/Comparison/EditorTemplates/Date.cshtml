@using ChameleonForms.Example.Views.Comparison.EditorTemplates
@{
    ViewData["type"] = "date";
    var attrs = ViewData.GetFieldAttrs();
    if (!string.IsNullOrEmpty(ViewData.ModelMetadata.EditFormatString))
    {
        attrs["data-val-format"] = ViewData.ModelMetadata.EditFormatString.Replace("{0:", "").Replace("}", "");
    }
}
@await Html.PartialAsync("~/Views/Comparison/EditorTemplates/HtmlInput.cshtml")
