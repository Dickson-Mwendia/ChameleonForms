<?xml version='1.0' encoding='UTF-8'?> 
<package>
  <metadata>
    <id>
       ChameleonForms
    </id>
    <version>
       1.0.0.0
    </version>
    <authors>
       Robert Moore, Matthew Davies
    </authors>
    <description>
      This library will shape-shift your forms experience in ASP.NET Core MVC.
      It makes it really easy to tersely output a form and easily apply a different template with a single line of code.
    </description>
    <projectUrl>
      https://github.com/MRCollective/ChameleonForms
    </projectUrl>
    <license type="expression">MIT</license>
    <icon>
      logo.png
    </icon>
    <tags>
       asp.net, mvc, forms, html, template
    </tags>
    <language>
       en-US
    </language>
    <dependencies>
      <group targetFramework=".NETCoreApp3.1">
        <dependency id="Humanizer" version="2.5.16" exclude="Build,Analyzers" />
        <dependency id="Microsoft.Extensions.DependencyInjection.Abstractions" version="3.1.3" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkReferences>
      <group targetFramework=".NETCoreApp3.1">
        <frameworkReference name="Microsoft.AspNetCore.App" />
      </group>
    </frameworkReferences>
    <contentFiles>
      <files include="any\any\wwwroot\lib\chameleonforms\*.js" buildAction="Content" />
    </contentFiles>
  </metadata>
  <files>
    <file src="ChameleonForms.Example\wwwroot\scripts\jquery.validate.unobtrusive.chameleon.js" target="contentFiles\any\any\wwwroot\lib\chameleonforms" />
    <file src="ChameleonForms\bin\Release\netcoreapp3.1\ChameleonForms.dll" target="lib\netcoreapp3.1" />
    <file src="ChameleonForms\bin\Release\netcoreapp3.1\ChameleonForms.pdb" target="lib\netcoreapp3.1" />
    <file src="ChameleonForms\bin\Release\netcoreapp3.1\ChameleonForms.XML" target="lib\netcoreapp3.1" />
    <file src="ChameleonForms.Core\bin\Release\netcoreapp3.1\ChameleonForms.Core.dll" target="lib\netcoreapp3.1" />
    <file src="ChameleonForms.Core\bin\Release\netcoreapp3.1\ChameleonForms.Core.pdb" target="lib\netcoreapp3.1" />
    <file src="ChameleonForms.Core\bin\Release\netcoreapp3.1\ChameleonForms.Core.XML" target="lib\netcoreapp3.1" />
    <file src="ChameleonForms.Templates\bin\Release\netcoreapp3.1\ChameleonForms.Templates.dll" target="lib\netcoreapp3.1" />
    <file src="ChameleonForms.Templates\bin\Release\netcoreapp3.1\ChameleonForms.Templates.pdb" target="lib\netcoreapp3.1" />
    <file src="ChameleonForms.Templates\bin\Release\netcoreapp3.1\ChameleonForms.Templates.XML" target="lib\netcoreapp3.1" />
    <file src="ChameleonForms\readme.txt" target="" />
    <file src="logo.png" target="" />
  </files>
</package>
